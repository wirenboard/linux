/dts-v1/;
#include "sun8i-r40.dtsi"
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/pwm/pwm.h>

#define PIN_PA &pio 0
#define PIN_PB &pio 1
#define PIN_PC &pio 2
#define PIN_PD &pio 3
#define PIN_PE &pio 4
#define PIN_PF &pio 5
#define PIN_PG &pio 6
#define PIN_PH &pio 7
#define PIN_PI &pio 8

/ {
	model = "Wiren Board 7 SoM Test (A40i)";
	compatible = "allwinner,sun8i-r40";

	aliases {
		ethernet0 = &gmac;
		ethernet1 = &emac;
		serial0 = &uart0;
		serial1 = &uart1;
		serial2 = &uart2;
		serial3 = &uart3;
		serial4 = &uart4;
		serial5 = &uart5;
		serial6 = &uart6;
		serial7 = &uart7;
		mmc0 = &mmc2;
		mmc1 = &mmc0;
		rtc0 = &rtc;
	};

	chosen {
		stdout-path = "serial0:115200n8";
	};

	regulators {
		compatible = "simple-bus";
		#address-cells = <1>;
		#size-cells = <0>;

		/* Wi-Fi on SoM */
		som_wifi_pwrseq: som_wifi_pwrseq {
			compatible = "mmc-pwrseq-simple";
			reset-gpios = <PIN_PC 3 GPIO_ACTIVE_LOW>; /* PC3*/
			// clocks = <&ccu CLK_OUTA>;
			// clock-names = "ext_clock";
		};
	};

	/* eMMC reset */
	emmc_pwrseq: emmc_pwrseq {
		compatible = "mmc-pwrseq-emmc";
		reset-gpios = <PIN_PC 24 GPIO_ACTIVE_LOW>;
	};

	wbmz_bat: wbmz-bat {
		compatible = "simple-battery";
		constant-charge-current-max-microamp = <600000>;
		voltage-min-design-microvolt = <2600000>;
		constant-charge-voltage-max-microvolt = <4100000>;
		status = "okay";
	};
};


&i2c1 {
	status = "okay";
};

&i2c3 {
	status = "okay";
	clock-frequency = <10000>;
};

&i2c3_pins {
	bias-pull-up;
};

